abel=\"whereFromLabel\"\r\n          :id=\"fromId\"\r\n        />\r\n      </div>\r\n      <div\r\n        v-if=\"showTo\"\r\n        tabindex=\"0\"\r\n        class=\"airport-lookup__field\"\r\n        @click=\"updateToFocus\"\r\n        @focus=\"updateToFocus\"\r\n      >\r\n        <InputField\r\n          v-model=\"toValue\"\r\n          :icon-left=\"\r\n            isMobile || (!isMobile && width > 412) || !toValue\r\n              ? [\r\n                  {\r\n                    'icon icon-airplane-arrive icon-color--grey3': !!airportTo,\r\n                    'icon icon-airplane-arrive icon-color--grey2': !airportTo,\r\n                  },\r\n                ]\r\n              : undefined\r\n          \"\r\n          name=\"airportTo\"\r\n          :label=\"whereToLabel\"\r\n          readonly=\"true\"\r\n          :id=\"toID\"\r\n        />\r\n      </div>\r\n      <div v-if=\"showTo\" class=\"airport-lookup__separator\">\r\n        <button\r\n          :disabled=\"!airportFrom || !airportTo\"\r\n          class=\"aiport-lookup__swap\"\r\n          aria-label=\"Swap origin and destination\"\r\n          role=\"button\"\r\n          @click=\"swap\"\r\n          @keyup:enter=\"swap\"\r\n        >\r\n          <i\r\n            :class=\"{\r\n              'icon icon-swap icon--xs icon-color--grey3':\r\n                !airportFrom || !airportTo,\r\n              'icon icon-swap icon--xs icon-color--grey1':\r\n                airportFrom || airportTo,\r\n            }\"\r\n          ></i>\r\n        </button>\r\n      </div>\r\n    </div>\r\n    <DropdownContainer\r\n      v-if=\"showResults\"\r\n      :fullScreen=\"isMobile\"\r\n      :open=\"showResults\"\r\n      :class=\"activeClass\"\r\n      @click:outside=\"showResults = false\"\r\n    >\r\n      <div\r\n        :class=\"[\r\n          {\r\n            'base-model__header': isMobile,\r\n          },\r\n          'airport-lookup__header',\r\n        ]\"\r\n      >\r\n        <div\r\n          v-if=\"isMobile && airportFrom && activeField === 'to'\"\r\n          class=\"airport-lookup__mobile-from-display\"\r\n        >\r\n          <span class=\"tag tag--airport\">{{ airportFrom.locIATA }}</span>\r\n          <i class=\"icon icon-airplane-right icon--xs icon-color--grey3\" />\r\n        </div>\r\n        <div class=\"airport-lookup__search\">\r\n          <InputField\r\n            v-if=\"!isMobile\"\r\n            ref=\"searchRef\"\r\n            :default-value=\"searchTermRef\"\r\n            icon-left=\"icon icon-airplane-depart icon-color--grey2\"\r\n            name=\"airportSearchTerm\"\r\n            :label=\"activeField === 'from' ? whereFromLabel : whereToLabel\"\r\n            @update:inputValue=\"(val) => (searchTermRef = val)\"\r\n          />\r\n          <input\r\n            v-if=\"isMobile\"\r\n            ref=\"searchRef\"\r\n            class=\"airport-lookup__mobile-input\"\r\n            type=\"text\"\r\n            name=\"airportSearchTerm\"\r\n            :placeholder=\"\r\n              activeField === 'from' ? whereFromLabel : whereToLabel\r\n            \"\r\n            @input=\"updateSearchTermField\"\r\n          />\r\n        </div>\r\n        <button\r\n          v-if=\"isMobile\"\r\n          class=\"button--text button--icon base-model__header-close m-r--8\"\r\n          aria-label=\"Dismiss modal\"\r\n          role=\"button\"\r\n          @click=\"showResults = false\"\r\n        >\r\n          <i class=\"icon icon-arrowleft icon--xs\" aria-hidden=\"true\"></i>\r\n        </button>\r\n      </div>\r\n      <AirportList\r\n        :nearby=\"displayNearby\"\r\n        :results=\"displayResults\"\r\n        :popularAirports=\"popularAirports\"\r\n        :isPlanner=\"isPlanner\"\r\n        :isTo=\"activeField === 'to'\"\r\n        :error=\"error\"\r\n        :loading=\"loading\"\r\n        :searchTerm=\"searchTermRef\"\r\n        :all-results-text=\"\r\n          popularAirports && popularAirports.length > 0\r\n            ? cmsProps?.popularDestinationsLabel.value ?? 'Popular destinations'\r\n            : cmsProps?.allAiportsLabel.value ?? 'All airports'\r\n        \"\r\n        :near-you-text=\"\r\n          cmsProps?.nearbyAirportsLabel.value\r\n            ? cmsProps?.nearbyAirportsLabel.value\r\n            : 'Near you'\r\n        \"\r\n        @itemSelected=\"selectAirport\"\r\n      />\r\n    </DropdownContainer>\r\n  </div>\r\n</template>\r\n\r\n<script setup lang=\"ts\">\r\nimport debounce from 'lodash/debounce';\r\nimport { Airport, AirportSearchApiResponse } from '../../types/ApiTypes';\r\nimport {\r\n  computed,\r\n  defineAsyncComponent,\r\n  ref,\r\n  watch,\r\n  nextTick,\r\n  inject,\r\n} from 'vue';\r\nimport InputField from '../InputField/InputField.vue';\r\nimport DropdownContainer from '../DropdownContainer/DropdownContainer.vue';\r\nimport useMatchMedia from '../../hooks/useMatchMedia';\r\nimport optiClient from '../../services/api';\r\nimport { useElementSize } from '@vueuse/core';\r\nimport { BookingWidgetContentInterface } from '../BookingWidget/types';\r\n\r\nconst cmsProps = inject<BookingWidgetContentInterface>('cms-props');\r\n\r\nlet dataLoaded = ref(false);\r\n\r\nconst AirportList = defineAsyncComponent(\r\n  () => import(\r\n    /* webpackChunkName: \"airportList\" */\r\n    '../AirportList/AirportList.vue'\r\n  ),\r\n);\r\n\r\ninterface Props {\r\n  showFrom?: boolean;\r\n  showTo?: boolean;\r\n  airportFrom?: Airport;\r\n  airportTo?: Airport;\r\n  showCode?: boolean;\r\n  isPlanner?: boolean;\r\n  hideNearby?: boolean;\r\n  fromAirportLabel?: string;\r\n}\r\nconst props = withDefaults(defineProps<Props>(), {\r\n  showFrom: true,\r\n  showTo: true,\r\n  airportFrom: undefined,\r\n  airportTo: undefined,\r\n  isPlanner: false,\r\n  hideNearby: false,\r\n  showCode: true,\r\n  fromAirportLabel: 'Where from?',\r\n});\r\n\r\ninterface Emits {\r\n  (e: 'update:airportFrom', airport: Airport): void;\r\n  (e: 'update:airportTo', airport: Airport): void;\r\n}\r\n\r\nconst emits = defineEmits<Emits>();\r\n\r\nconst searchTermRef = ref('');\r\nconst showResults = ref(false);\r\nconst activeField = ref<'from' | 'to' | ''>('');\r\nconst results = ref<Airport[]>([]);\r\nconst nearby = ref<Airport[]>([]);\r\nconst error = ref('');\r\nconst loading = ref(true);\r\nconst searchRef = ref();\r\nconst el = ref(null);\r\nconst isMobile = useMatchMedia('(max-width: 416px)');\r\nconst { width } = useElementSize(el);\r\nconst fromId = Math.random().toString(36).substring(2, 12)\r\nconst toID = Math.random().toString(36).substring(2, 12)\r\n\r\nconst debouncedSearch = debounce(search, 500);\r\n\r\nconst whereFromLabel = computed(() => {\r\n  return cmsProps?.fromAirportLabel?.value\r\n    ? cmsProps?.fromAirportLabel?.value\r\n    : props.fromAirportLabel;\r\n});\r\n\r\nconst whereToLabel = computed(() => {\r\n  return cmsProps?.toAirportLabel?.value\r\n    ? cmsProps?.toAirportLabel?.value\r\n    : 'Where to?';\r\n});\r\n\r\nasync function search(term: string) {\r\n  loading.value = true;\r\n  error.value = '';\r\n\r\n  try {\r\n    const endpoint = props.isPlanner\r\n      ? optiClient.planner.getApiV1PlannerAirportsSearch(\r\n          term,\r\n          term?.length > 0 ? 'false' : 'true',\r\n          '800',\r\n          '',\r\n          term?.length > 0 ? 'true' : 'false',\r\n        )\r\n      : optiClient.bookings.getApiV1BookingsAirportsSearch(\r\n          term,\r\n          term?.length > 0 ? 'false' : 'true',\r\n          '800',\r\n          '',\r\n          term?.length > 0 ? 'true' : 'false',\r\n        );\r\n    const res: AirportSearchApiResponse = await endpoint;\r\n    results.value = res?.data?.[0] ?? [];\r\n    nearby.value = res?.data?.[1] ?? [];\r\n    dataLoaded.value = true;\r\n  } catch (err) {\r\n    error.value =\r\n      'We are unable to return results. If the issue persists, please contact our support.';\r\n  } finally {\r\n    loading.value = false;\r\n  }\r\n}\r\n\r\nwatch(\r\n  () => showResults.value,\r\n  (open) => {\r\n    if (!open) {\r\n      const parent = document.querySelector('.booking-widget--large');\r\n      const nextFocusTarget = parent?.querySelector(\r\n        '.airport-lookup--desktop',\r\n      ) as HTMLButtonElement;\r\n      if (nextFocusTarget) {\r\n        nextFocusTarget.focus();\r\n      }\r\n    }\r\n    if (open) {\r\n      nextTick(() => {\r\n        searchRef.value?.focus();\r\n      });\r\n    }\r\n  },\r\n);\r\n\r\nconst fromValue = computed(() => {\r\n  if (props.airportFrom) {\r\n    if (props.showCode) {\r\n      return `${props.airportFrom.locCity} ${props.airportFrom.locIATA}`;\r\n    } else {\r\n      return `${props.airportFrom.locCity}`;\r\n    }\r\n  }\r\n  return '';\r\n});\r\n\r\nconst toValue = computed(() => {\r\n  if (props.airportTo) {\r\n    if (props.showCode) {\r\n      return `${props.airportTo.locCity} ${props.airportTo.locIATA}`;\r\n    } else {\r\n      return `${props.airportTo.locCity}`;\r\n    }\r\n  }\r\n  return '';\r\n});\r\n\r\nconst popularAirports = computed(() => {\r\n  if (\r\n    typeof props.airportFrom !== 'undefined' &&\r\n    activeField.value === 'to' &&\r\n    props.showTo !== false\r\n  ) {\r\n    const popularDestinations = props.airportFrom.popularDestinations;\r\n    return popularDestinations.split(',');\r\n  }\r\n  return null;\r\n});\r\n\r\nconst displayFilterPredicate = computed(() => (airport: Airport) => {\r\n  if (\r\n    typeof props.airportFrom !== 'undefined' &&\r\n    activeField.value === 'to' &&\r\n    airport.locIATA === props.airportFrom.locIATA\r\n  ) {\r\n    return false;\r\n  }\r\n  return true;\r\n});\r\n\r\nconst displayNearby = computed(() => {\r\n  if (props.hideNearby) {\r\n    return [];\r\n  } else {\r\n    return nearby.value.filter(displayFilterPredicate.value);\r\n  }\r\n});\r\n\r\nconst filteredResults = computed(() => {\r\n  return results.value.filter(displayFilterPredicate.value);\r\n});\r\n\r\nconst displayResults = computed(() => {\r\n  if (!popularAirports.value?.l